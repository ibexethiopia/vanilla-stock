# Prevent upload of .env file, which contains sensitive environment variables
.env
.env.backup
.env.local
.env.testing
.env.example

# Prevent IDE helpers and various editor files from being tracked
.phpunit.result.cache
/.idea
/.vscode
/.phpstorm.meta.php

# Prevent coverage reports from being tracked
coverage/

# Ignore compiled classes
/vendor/composer/autoload_classmap.php
/vendor/composer/autoload_static.php
/vendor/composer/autoload_files.php

# Ignore system files
.DS_Store
Thumbs.db
*.log
*.swp
*.swo

# Ignore cached config and routes
/bootstrap/cache/*.php

# Ignore composer.lock (optional, depending on your team preference)
composer.lock

# Ignore npm/yarn lock files (optional, depending on your team preference)
package-lock.json
yarn.lock
pnpm-lock.yaml

# Prevent Laravel Mix or Vite-related files from being committed
/public/mix-manifest.json
/public/hot
!public/build/
!public/build/manifest.json
!public/build/assets/*


# Ignore backup files and temp files
*.bak
*.tmp
*.temp

# Ignore PHPUnit results and coverage files
.phpunit.result.cache
coverage/
